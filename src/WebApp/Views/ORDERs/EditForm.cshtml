﻿@model WebApp.Models.ORDER
<!-- begin master section -->
<div  class="main-form">
<!--begin order form  -->
<form id="order_form" method="post" novalidate="novalidate" >
@Html.AntiForgeryToken()
   @Html.HiddenFor(x=>x.ID)
