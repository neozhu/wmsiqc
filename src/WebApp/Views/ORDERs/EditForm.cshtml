 <div class="form-group form-group-sm">
      <div class="row">

        <!--出货单号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.ORDERKEY)</span> </label>
			@Html.EditorFor(x => x.ORDERKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.ORDERKEY) } })
      
         </div>

        <!--仓库号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.WHSEID)</span> </label>
			@Html.EditorFor(x => x.WHSEID, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.WHSEID) } })
      
         </div>

        <!--批次号-->
