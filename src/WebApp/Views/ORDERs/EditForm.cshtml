            <label class="control-label">@Html.DisplayNameFor(model => model.EXTERNORDERKEY) </label>
			@Html.EditorFor(x => x.EXTERNORDERKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.EXTERNORDERKEY) } })
      
