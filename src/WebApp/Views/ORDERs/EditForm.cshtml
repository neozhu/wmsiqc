            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.ORDERDATE)</span> </label>
            <div class="input-group">
			  @Html.EditorFor(x => x.ORDERDATE, 
                         new { htmlAttributes = new { @class = "form-control datepicker hasDatepicker",
                               placeholder = Html.DisplayNameFor(x => x.ORDERDATE ) } })
               <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
			 </div>
         </div>

