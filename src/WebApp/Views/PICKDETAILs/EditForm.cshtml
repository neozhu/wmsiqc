﻿@model WebApp.Models.PICKDETAIL
<!-- begin master section -->
<div  class="main-form">
<!--begin pickdetail form  -->
<form id="pickdetail_form" method="post" novalidate="novalidate" >
@Html.AntiForgeryToken()
   @Html.HiddenFor(x=>x.ID)
 <fieldset>
 <div class="form-group form-group-sm">
      <div class="row">

        <!--拣货单号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.PICKDETAILKEY)</span> </label>
			@Html.EditorFor(x => x.PICKDETAILKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.PICKDETAILKEY) } })
      
         </div>

        <!--仓库号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.WHSEID)</span> </label>
			@Html.EditorFor(x => x.WHSEID, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.WHSEID) } })
      
         </div>

        <!--出货单号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.ORDERKEY)</span> </label>
			@Html.EditorFor(x => x.ORDERKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.ORDERKEY) } })
      
         </div>

        <!--入库批次号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.EXTERNRECEIPTKEY) </label>
			@Html.EditorFor(x => x.EXTERNRECEIPTKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.EXTERNRECEIPTKEY) } })
      
         </div>

        <!--货主-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.STORERKEY)</span> </label>
			@Html.EditorFor(x => x.STORERKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.STORERKEY) } })
      
         </div>

        <!--状态-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.STATUS) </label>
			@Html.EditorFor(x => x.STATUS, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.STATUS) } })
      
         </div>
	   </div>
    </div>
 <div class="form-group form-group-sm">
      <div class="row">

        <!--物料编号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.SKU)</span> </label>
			@Html.EditorFor(x => x.SKU, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.SKU) } })
      
         </div>

        <!--物料名称-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.SKUNAME) </label>
			@Html.EditorFor(x => x.SKUNAME, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.SKUNAME) } })
      
         </div>

		<!--数量-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.QTY)</span> </label>
            @Html.EditorFor(x => x.QTY, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.QTY) } })

        </div>

        <!--单位-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.UMO)</span> </label>
			@Html.EditorFor(x => x.UMO, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.UMO) } })
      
         </div>

		<!--件数-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.CASECNT)</span> </label>
            @Html.EditorFor(x => x.CASECNT, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.CASECNT) } })

        </div>

        <!--库位(良品区/不良品区)-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOC) </label>
			@Html.EditorFor(x => x.LOC, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOC) } })
      
         </div>
	   </div>
    </div>
 <div class="form-group form-group-sm">
      <div class="row">

        <!--批号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOT) </label>
			@Html.EditorFor(x => x.LOT, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOT) } })
      
         </div>

        <!--LPN-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LPN) </label>
			@Html.EditorFor(x => x.LPN, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LPN) } })
      
         </div>

        <!--供应商代码-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.SUPPLIERCODE) </label>
			@Html.EditorFor(x => x.SUPPLIERCODE, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.SUPPLIERCODE) } })
      
         </div>

        <!--供应商名称-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.SUPPLIERNAME) </label>
			@Html.EditorFor(x => x.SUPPLIERNAME, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.SUPPLIERNAME) } })
      
         </div>

        <!--收货单号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.RECEIPTKEY)</span> </label>
			@Html.EditorFor(x => x.RECEIPTKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.RECEIPTKEY) } })
      
         </div>

        <!--订单编号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.POKEY) </label>
			@Html.EditorFor(x => x.POKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.POKEY) } })
      
         </div>
	   </div>
    </div>
 <div class="form-group form-group-sm">
      <div class="row">

       <!--收货单日期-->
       <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.RECEIPTDATE)</span> </label>
            <div class="input-group">
			  @Html.EditorFor(x => x.RECEIPTDATE, 
                         new { htmlAttributes = new { @class = "form-control datepicker hasDatepicker",
                               placeholder = Html.DisplayNameFor(x => x.RECEIPTDATE ) } })
               <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
			 </div>
         </div>

		<!--栈板数-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.PALLET)</span> </label>
            @Html.EditorFor(x => x.PALLET, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.PALLET) } })

        </div>

        <!--包装-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.PACKKEY) </label>
			@Html.EditorFor(x => x.PACKKEY, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.PACKKEY) } })
      
         </div>

		<!--毛重(KG)-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.GROSSWGT)</span> </label>
            @Html.EditorFor(x => x.GROSSWGT, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.GROSSWGT) } })

        </div>

		<!--净重(KG)-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.NETWGT)</span> </label>
            @Html.EditorFor(x => x.NETWGT, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.NETWGT) } })

        </div>

		<!--体积(M)-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.CUBE)</span> </label>
            @Html.EditorFor(x => x.CUBE, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.CUBE) } })

        </div>
	   </div>
    </div>
 <div class="form-group form-group-sm">
      <div class="row">

        <!--工厂名称-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE1) </label>
			@Html.EditorFor(x => x.LOTTABLE1, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOTTABLE1) } })
      
         </div>

        <!--公司名称-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE2) </label>
			@Html.EditorFor(x => x.LOTTABLE2, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOTTABLE2) } })
      
         </div>

        <!--入库外箱标签-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE3) </label>
			@Html.EditorFor(x => x.LOTTABLE3, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOTTABLE3) } })
      
         </div>

        <!--发货单号-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE4) </label>
			@Html.EditorFor(x => x.LOTTABLE4, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOTTABLE4) } })
      
         </div>

        <!--出库外箱标签-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE5) </label>
			@Html.EditorFor(x => x.LOTTABLE5, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOTTABLE5) } })
      
         </div>

		<!--LOTTABLE6-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE6) </label>
            @Html.EditorFor(x => x.LOTTABLE6, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.LOTTABLE6) } })

        </div>
	   </div>
    </div>
 <div class="form-group form-group-sm">
      <div class="row">

		<!--LOTTABLE7-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE7) </label>
            @Html.EditorFor(x => x.LOTTABLE7, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.LOTTABLE7) } })

        </div>

       <!--LOTTABLE7-->
       <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE8) </label>
            <div class="input-group">
			  @Html.EditorFor(x => x.LOTTABLE8, 
                         new { htmlAttributes = new { @class = "form-control datepicker hasDatepicker",
                               placeholder = Html.DisplayNameFor(x => x.LOTTABLE8 ) } })
               <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
			 </div>
         </div>

     
       <!--出货单ID-->
       <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.ORDERID)</span> </label>
   	        @Html.DropDownListFor(model => model.ORDERID, ViewBag.ORDERID as List<SelectListItem>, htmlAttributes: new { @class = "form-control" }) 
       </div>
	    
   </div>
  </div>
</fieldset>
</form>
<!-- end pickdetail form  -->
</div>

<!-- end master section -->
