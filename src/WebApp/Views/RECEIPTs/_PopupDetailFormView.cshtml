  function hasmodified() {
    if (hasChanged) return hasChanged;
    if (!receiptdetails_endEditing()) {
      return true;
    }
    const receiptdetails = $receiptdetailsdg.datagrid("getChanges");
    if (MODELSTATE == 'Modified' && receiptdetails.length > 0) {
      return true;
    }
    return false;

  }


